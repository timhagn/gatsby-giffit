{
  "name": "gatsby-plugin-giffit",
  "version": "0.0.2",
  "description": "Gatsby Plugin and Transformer to easily work with optimized animated GIFs.",
  "author": "Tim Hagn <mail@timhagn.com>",
  "bugs": {
    "url": "https://github.com/timhagn/giffit/issues"
  },
  "dependencies": {
    "@babel/runtime": "^7.7.6",
    "async": "^3.1.0",
    "chokidar": "^3.3.0",
    "cli-progress": "^3.4.0",
    "fs-extra": "^8.1.0",
    "gif-frames": "^1.0.1",
    "gifsicle-stream": "^1.0.0",
    "lodash": "^4.17.15",
    "probe-image-size": "^5.0.0",
    "progress": "^2.0.3",
    "prop-types": "^15.7.2",
    "temp": "^0.9.1",
    "webp-converter": "^2.2.3"
  },
  "peerDependencies": {
    "react": "16.x",
    "react-dom": "16.x"
  },
  "homepage": "https://github.com/timhagn/giffit/tree/master/packages/gatsby-plugin-giffit#readme",
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "image",
    "gif",
    "webp",
    "converter",
    "gif2webp",
    "gifsicle"
  ],
  "license": "MIT",
  "main": "index.js",
  "repository": "https://github.com/timhagn/giffit/tree/master/packages/gatsby-plugin-giffit",
  "scripts": {
    "build": "npm run format && babel src --out-dir . --ignore **/__tests__",
    "format": "prettier --write \"src/*.js\"",
    "prepare": "cross-env NODE_ENV=production npm run build",
    "test": "jest --env=jsdom",
    "test:update": "jest --env=jsdom -u --coverage",
    "watch": "babel -w src --out-dir . --ignore **/__tests__"
  }
}
